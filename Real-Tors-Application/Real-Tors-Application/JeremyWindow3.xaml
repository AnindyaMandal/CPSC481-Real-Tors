<Page x:Class="Real_Tors_Application.JeremyWindow3"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Real_Tors_Application"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
      Title="JeremyWindow3"
      Background="White">

	<Grid>
        <StackPanel x:Name="navBarStackPanel" Orientation="Horizontal" Height="100" VerticalAlignment="Top" Width="1920" Background="#FF7B82A0">
            <Button x:Name="homeButton" Content="Home" Width="384" FontSize="40" Background="#FF7B82A0" Click="homeButton_Click"/>
            <Button x:Name="aboutButton" Content="About" Width="384" FontSize="40" Background="#FF7B82A0"/>
            <Button x:Name="teamButton" Content="Our team" Width="384" FontSize="40" Background="#FF7B82A0"/>
            <Button x:Name="contactButton" Content="Contact Us" Width="384" FontSize="40" Background="#FF7B82A0"/>
            <Button x:Name="profileButton" Content="My Profile" Width="384" FontSize="40" Background="#FF7B82A0" Visibility="Collapsed" Click="profileButton_Click"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal" Margin="0, 100, 0 ,0">
            <StackPanel HorizontalAlignment="Left" Orientation="Horizontal" Height="980" Margin="0, 0, 0, 0" Background="White">
                
                <ScrollViewer>
                    <StackPanel Name="FilterPanel" Width="400">

                        <Label FontSize="36" FontWeight="Bold" Padding="24" Background="#FFAAAAAA" HorizontalContentAlignment="Center">Filters</Label>
                        <Label Height="10" />
                        <StackPanel Orientation="Horizontal">
                            <Label FontSize="26" FontWeight="Bold" Padding="24 12 48 12" Width="300">Home Type:</Label>
                            <Button Name="OpenHomeTypes" Click="toggleTypes">
                                <Image x:Name="dropdown_btn_image" Source="sortdown_icon.png"/>
                            </Button>

						</StackPanel>


                        <StackPanel Name="HomeTypesSelect" Margin="14" HorizontalAlignment="Center" Visibility="Visible">
                            <RadioButton Name="SingleButt" FontSize = "26" Padding="0 6" Content="Single Family" Click="SingleFamily_Click"/>
                            <RadioButton Name="DuplexButt" FontSize = "26" Padding="0 6" Content="Duplex" Click="Duplex_Click"/>
                            <RadioButton Name="TriplexButt" FontSize = "26" Padding="0 6" Content="Triplex" Click="Triplex_Click"/>
                            <RadioButton Name="TownhomeButt" FontSize = "26" Padding="0 6" Content="Townhome" Click="Townhome_Click"/>
                            <RadioButton Name="ApartButt" FontSize = "26" Padding="0 6" Content="Apartment" Click="Apartment_Click"/>
                            <RadioButton Name="LoftButt" FontSize = "26" Padding="0 6" Content="Loft" Click="Loft_Click"/>
                        </StackPanel>

                        <Label Content="Please Press 'Enter' to Update" Margin="25" FontSize="25" Foreground="#FF55EA8C"/>
                        
                        <Label Content="Price" FontWeight="Bold" Padding="24 12 0 12" FontSize="26"/>
                        <TextBox Name="lowPrice" IsReadOnly="False" Background="#eee" Margin="20 0 50 20" FontSize="25" KeyDown="enterPriceLow"/>
                        <Label Content="to" FontSize="22" Padding="24 0 0 10"/>
                        <TextBox Name="highPrice" IsReadOnly="False" Background="#eee" Margin="20 10 50 20" FontSize="25" KeyDown="enterPriceHigh"/>


                        <Label Content="Size (sq ft)" FontWeight="Bold" Padding="24 24 0 12" FontSize="26"/>
                        <TextBox Name ="lowSize" IsReadOnly="False" Background="#eee" Margin="20 0 50 20" FontSize="25" KeyDown="enterSizeLow"/>
                        <Label Content="to" FontSize="22" Padding="24 0 0 10"/>
                        <TextBox Name ="highSize" IsReadOnly="False" Background="#eee" Margin="20 10 50 20" FontSize="25" KeyDown="enterSizeHigh"/>


                        <Label Content="# of Beds" FontWeight="Bold" Padding="24 24 0 12" FontSize="26"/>
                        <TextBox Name="lowBed" IsReadOnly="False" Background="#eee" Margin="20 0 250 20" FontSize="25" KeyDown="enterBedLow"/>
                        <Label Content="to" FontSize="22" Padding="24 0 0 10"/>
                        <TextBox Name="highBed" IsReadOnly="False" Background="#eee" Margin="20 10 250 20" FontSize="25" KeyDown="enterBedHigh"/>

                        <Label Content="# of Baths" FontWeight="Bold" Padding="24 24 0 12" FontSize="26"/>
                        <TextBox Name="lowBath" IsReadOnly="False" Background="#eee" Margin="20 0 250 20" FontSize="25" KeyDown="enterBathLow"/>
                        <Label Content="to" FontSize="22" Padding="24 0 0 10"/>
                        <TextBox Name="highBath" IsReadOnly="False" Background="#eee" Margin="20 10 250 20" FontSize="25" KeyDown="enterBathHigh"/>

                        <Label Content="Year Built" FontWeight="Bold" Padding="24 24 0 12" FontSize="26"/>
                        <TextBox Name="lowYear" IsReadOnly="False" Background="#eee" Margin="20 0 200 20" FontSize="25" KeyDown="enterYearLow"/>
                        <Label Content="to" FontSize="22" Padding="24 0 0 10"/>
                        <TextBox Name="highYear" IsReadOnly="False" Background="#eee" Margin="20 10 200 20" FontSize="25" KeyDown="enterYearHigh"/>

                        <Label FontSize="26" FontWeight="Bold" HorizontalAlignment="Left" Padding="24 12 48 12" Width="300">Amenities:</Label>
                        <StackPanel Orientation="Horizontal">
                            <TextBox Name ="AmenitiesInput" IsReadOnly="False" Background="#eee" Width="250" Margin="24 4" FontSize="25" KeyDown="EnterAmenity"/>
                            <Button Name="AddAmenities" Content="Add" FontSize="24" Margin="8" Padding="20 0" HorizontalAlignment="Right" Click="Add_Amenity_Click"/>
                        </StackPanel>
                        <WrapPanel Orientation="Horizontal">
                            <Label Name="Amenity1" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed" MouseLeftButtonDown="DeleteAmenity"/>
                            <Label Name="Amenity2" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed" MouseLeftButtonDown="DeleteAmenity"/>
                            <Label Name="Amenity3" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed" MouseLeftButtonDown="DeleteAmenity"/>
                            <Label Name="Amenity4" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed" MouseLeftButtonDown="DeleteAmenity"/>
                            <Label Name="Amenity5" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed" MouseLeftButtonDown="DeleteAmenity"/>
                        </WrapPanel>

                        <Label Height="20" />
                        <Label FontSize="26" FontWeight="Bold" HorizontalAlignment="Left" Padding="24 12 48 12" Width="300">Neighbourhoods:</Label>
                        <StackPanel Orientation="Horizontal">
                            <TextBox Name ="NeighbourhoodInput" IsReadOnly="False" Background="#eee" Width="250" Margin="24 4" FontSize="25" KeyDown="EnterNeighbour"/>
                            <Button Name="AddNeighbourhood" Content="Add" FontSize="24" Margin="8" Padding="20 0" HorizontalAlignment="Right" Click="Add_Neighbourhood_Click"/>
                        </StackPanel>
                        <WrapPanel Orientation="Horizontal">
                            <Label Name="Neighbour1" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed" MouseLeftButtonDown="DeleteNeighbour"/>
                            <Label Name="Neighbour2" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed" MouseLeftButtonDown="DeleteNeighbour"/>
                            <Label Name="Neighbour3" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed" MouseLeftButtonDown="DeleteNeighbour"/>
                            <Label Name="Neighbour4" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed" MouseLeftButtonDown="DeleteNeighbour"/>
                            <Label Name="Neighbour5" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed" MouseLeftButtonDown="DeleteNeighbour"/>
                            <Label Name="Neighbour6" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed" MouseLeftButtonDown="DeleteNeighbour"/>
                        </WrapPanel>
                        <Label Height="50" />

                        <Grid Height="400"/>

                    </StackPanel>
                </ScrollViewer>
                <Button Content="||" FontSize="30" VerticalAlignment="Center" Click="ToggleFilter"  Width="20"/>
            </StackPanel>
            <Grid x:Name="expandedListingGrid" VerticalAlignment="Top" Background="LightGray" Width="1480" MouseLeftButtonDown="exitExpanded" >
                <Grid Name="expandedListingPopOut" HorizontalAlignment="Center" Background="White" Margin="50" MouseEnter="enterPopUp" MouseLeave="exitPopUp">
                    <ScrollViewer x:Name="expandedListingScrollViewer" VerticalAlignment="Top"  Height ="880" Width="1220" VerticalScrollBarVisibility="Auto">

                        <Grid VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0 0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="100"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>

					<Button Name="BackIfSimilar" Visibility="Collapsed" Grid.Row="1" Content="Back" Width="200" Margin="80, 0, 0, 0" FontSize="30" HorizontalAlignment="Left" VerticalAlignment="Bottom" Height="75" Click="BackToSelection"/>
                    <Label Name="Address" Margin="530,30,0,0" FontWeight="Black" Height="80" FontSize="50" Grid.Row="1"  Content="{Binding Path=address[0], UpdateSourceTrigger=PropertyChanged}"></Label>


                            <StackPanel Orientation="Vertical" Grid.Row="2">

                                <Grid HorizontalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Name="Column1" Width="500"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid Grid.Column="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="70"/>
                                            <RowDefinition Height="70"/>
                                            <RowDefinition Height="70"/>
                                            <RowDefinition Height="70"/>
                                            <RowDefinition Height="70"/>
                                            <RowDefinition Height="70"/>
                                            <RowDefinition Height="70"/>
                                            <RowDefinition Height="70"/>
                                        </Grid.RowDefinitions>

                                        <StackPanel Orientation="Horizontal" Grid.Row="0" Margin="30 0">
                                            <Label x:Name="sizeTextBlock" Content="Size:" VerticalAlignment="Top" FontSize="28" FontWeight="Bold" />
                                            <Label Name="SizeNumber"  Margin="20 0" Height="50"  FontSize="28"  VerticalAlignment="Top" Content="{Binding Path=sizenum[0], UpdateSourceTrigger=PropertyChanged}"></Label>

                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" Grid.Row="1" Margin="30 0">
                                            <Label x:Name="bedsTextBlock" Content="Beds:" VerticalAlignment="Top" FontSize="28" FontWeight="Bold"/>
                                            <Label Name="BedNumber"  Margin="20 0" Height="50"  FontSize="28"  VerticalAlignment="Top" Content="{Binding Path=bednum[0], UpdateSourceTrigger=PropertyChanged}"></Label>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" Grid.Row="2" Margin="30 0">
                                            <Label x:Name="bathroomsTextBlock" Content="Bathrooms:" VerticalAlignment="Top" FontSize="28" FontWeight="Bold" />
                                            <Label Name="BathNumber"  Margin="20 0" Height="50"  FontSize="28"  VerticalAlignment="Top" Content="{Binding Path=bathnum[0], UpdateSourceTrigger=PropertyChanged}"></Label>

                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" Grid.Row="3" Margin="30 0">
                                            <Label x:Name="priceTextBlock" Content="Price:" VerticalAlignment="Top" FontSize="28" FontWeight="Bold"/>
                                            <Label Name="PriceNumber"  Margin="20 0 " Height="50"  FontSize="28"  VerticalAlignment="Top" Content="{Binding Path=pricenum[0], UpdateSourceTrigger=PropertyChanged}"></Label>

                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" Grid.Row="4" Margin="30 0">
                                            <Label x:Name="neighbourhoodTextBlock" Content="Neighbourhood:" VerticalAlignment="Top" FontSize="28" FontWeight="Bold" />
                                            <Label Name="Neighbourhood"  Margin="20 0" Height="50" FontSize="28" VerticalAlignment="Top" Content="{Binding Path=neighbourhood[0], UpdateSourceTrigger=PropertyChanged}"></Label>

                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" Grid.Row="5" Margin="30 0">
                                            <Label x:Name="moveInReadyTextBlock" Content="Move In Ready:" VerticalAlignment="Top" FontSize="28" FontWeight="Bold" />
                                            <Viewbox Margin="20, 10, 0, 0" Height="30"  VerticalAlignment="Top">
                                                <CheckBox IsChecked="True" IsHitTestVisible="False" Focusable="False" />
                                            </Viewbox>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" Grid.Row="6" Margin="30 0">
                                            <Label x:Name="amenitiesTextBlock" Content="Amenities:" VerticalAlignment="Top" FontSize="28" FontWeight="Bold" />
                                            <WrapPanel Orientation="Horizontal">
                                                <Label Name="AmenityF1" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed"/>
                                                <Label Name="AmenityF2" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed"/>
                                                <Label Name="AmenityF3" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed"/>
                                                <Label Name="AmenityF4" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed"/>
                                                <Label Name="AmenityF5" FontSize="20" Padding="8 2 12 2" Margin="12" Background="LightGray" Visibility="Collapsed"/>
                                            </WrapPanel>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" Grid.Row="7" Margin="30 0">
                                            <Label x:Name="descriptionTextBlock" Content="Description:" VerticalAlignment="Top" FontSize="28" FontWeight="Bold" />
                                        </StackPanel>

                                    </Grid>
                                    <StackPanel Orientation="Horizontal" Grid.Column="1" HorizontalAlignment="Center">


										<Button Name="changeMainImageLeft1" Width="30" Height="45" Margin="5 0" Click="changeMainImageLeft">
											<Image Source="left.png"/>
										</Button>
                                        <Grid Width="600">
                                            <Image Name="MainHouseImage" Height ="400 " Source="houseImg1.jpg"/>
                                        </Grid>
										

										<Button Name="changeMainImageRight1" Width="30" Height="45" Margin="5 0" Click="changeMainImageRight">
											<Image Source="right.png"/>
										</Button>
										
                                    </StackPanel>


                                </Grid>
                                <TextBox Name="DescriptionText" Margin="100 12" Height="130" Width="Auto" AcceptsReturn="True" TextWrapping="Wrap" FontSize="24" IsReadOnly="True"/>
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0 50 0 0">
                                    <Image Name="FavoritedListing" Source="HeartIconEmpty.png" Height="163" Width="85" MouseLeftButtonDown="ChangeFavorite"/>
                                    <Button x:Name="saveForLater" Content="Save For Later" Padding="8" Margin="20 0 20 0" Height="100" Width="320" FontSize="40" Background="#d71616" Foreground="White" Click="btn_saveForLater_Click"/>
                                    <Button x:Name="contactRealtor" Content="Contact Realtor!" Padding="8" Margin="20 0 20 0" Height="100" Width="320" FontSize="40" Background="#296dcf" Foreground="White" Click="btn_contactRealtor_Click"/>
                                </StackPanel>

                                <Label x:Name="similarListingsTextBlock" Content="Similar Listings:" VerticalAlignment="Top" FontSize="28" FontWeight="Bold"  Margin="20 0 20 0" />

                                <ScrollViewer Name ="SimilarListingsScroll" HorizontalAlignment ="Left" HorizontalScrollBarVisibility="Visible" Height ="400" Width ="1150" VerticalScrollBarVisibility="Disabled" Margin="25 50">
                                    <StackPanel Orientation="Horizontal">

                                        <Grid Name ="Listing1" MouseLeftButtonDown="expandListing">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="350"></ColumnDefinition>
                                                <ColumnDefinition Width="50"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="250"/>
                                                <RowDefinition Height="100"/>
                                                <RowDefinition Height="50"/>
                                            </Grid.RowDefinitions>

                                            <Rectangle Grid.Row="0" Grid.Column="0" Fill="#eee" Height="250" Width="350" HorizontalAlignment="Left" Stroke="Black" VerticalAlignment="Top"/>
                                            <Rectangle Grid.Row="1" Grid.Column="0" Fill="#fff" Height="100" Width="350" HorizontalAlignment="Left" Stroke="Black" VerticalAlignment="Top"/>
                                            <Image Name="HouseImage1" Grid.Column="0" Margin="25"/>
                                            <Label Name="PriceNum1" Grid.Row="1" Grid.Column="0" FontSize="25" FontWeight="Black" Margin="8" HorizontalAlignment="Right" Content="{Binding Path=price[0], UpdateSourceTrigger=PropertyChanged}"></Label>
                                            <Label Name="Neighbourhood1" Grid.Row="1" Grid.Column="0" FontSize="25" FontWeight="Bold" Margin="8" HorizontalAlignment="Left" Content="{Binding Path=neighbourhood[0], UpdateSourceTrigger=PropertyChanged}"></Label>


                                            <Grid Name ="BedBath1" Grid.Row="1" Grid.Column ="0" HorizontalAlignment="center" VerticalAlignment="Bottom" Margin="8">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="30"/>
                                                    <ColumnDefinition Width="50"/>
                                                    <ColumnDefinition Width="40"/>
                                                    <ColumnDefinition Width="50"/>
                                                    <ColumnDefinition Width="150"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="50"/>
                                                </Grid.RowDefinitions>

                                                <Label Name="BedNumber1" Grid.Column="0" FontSize="30" FontWeight="Black" HorizontalAlignment="Right"  VerticalAlignment="Bottom" Foreground="#ff9900"></Label>
                                                <Image Name="BedIcon1" Grid.Column="1" HorizontalAlignment="Left" Source="BedroomIcon.png" Margin="10 5 10 0"></Image>
                                                <Label Name="BathNumber1" Grid.Column="2" FontSize="30" FontWeight="Black" HorizontalAlignment="Right" VerticalAlignment="Bottom" Foreground="#55aaff"></Label>
                                                <Image Name="BathIcon1" Grid.Column="3" HorizontalAlignment="Left" Source="BathroomIcon.png" Margin="10 5 10 0"></Image>
                                                <Label Name="SizeNumber1" Grid.Column="4" FontSize="30" FontWeight="Normal" HorizontalAlignment="Right" VerticalAlignment="Bottom" Foreground="Black"></Label>
                                            </Grid>


                                        </Grid>

                                        <Grid Name ="Listing2" MouseLeftButtonDown="expandListing">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="350"></ColumnDefinition>
                                                <ColumnDefinition Width="50"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="250"/>
                                                <RowDefinition Height="100"/>
                                                <RowDefinition Height="50"/>
                                            </Grid.RowDefinitions>

                                            <Rectangle Grid.Row="0" Grid.Column="0" Fill="#eee" Height="250" Width="350" HorizontalAlignment="Left" Stroke="Black" VerticalAlignment="Top"/>
                                            <Rectangle Grid.Row="1" Grid.Column="0" Fill="#fff" Height="100" Width="350" HorizontalAlignment="Left" Stroke="Black" VerticalAlignment="Top"/>
                                            <Image Name="HouseImage2" Grid.Column="0" Margin="25"/>
                                            <Label Name="PriceNum2" Grid.Row="1" Grid.Column="0" FontSize="25" FontWeight="Black" Margin="8" HorizontalAlignment="Right" Content="{Binding Path=price[0], UpdateSourceTrigger=PropertyChanged}"></Label>
                                            <Label Name="Neighbourhood2" Grid.Row="1" Grid.Column="0" FontSize="25" FontWeight="Bold" Margin="8" HorizontalAlignment="Left" Content="{Binding Path=neighbourhood[0], UpdateSourceTrigger=PropertyChanged}"></Label>

                                            <Grid Name ="BedBath2" Grid.Row="1" Grid.Column ="0" HorizontalAlignment="center" VerticalAlignment="Bottom" Margin="8">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="30"/>
                                                    <ColumnDefinition Width="50"/>
                                                    <ColumnDefinition Width="40"/>
                                                    <ColumnDefinition Width="50"/>
                                                    <ColumnDefinition Width="150"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="50"/>
                                                </Grid.RowDefinitions>

                                                <Label Name="BedNumber2" Grid.Column="0" FontSize="30" FontWeight="Black" HorizontalAlignment="Right"  VerticalAlignment="Bottom" Foreground="#ff9900"></Label>
                                                <Image Name="BedIcon2" Grid.Column="1" HorizontalAlignment="Left" Source="BedroomIcon.png" Margin="10 5 10 0"></Image>
                                                <Label Name="BathNumber2" Grid.Column="2" FontSize="30" FontWeight="Black" HorizontalAlignment="Right" VerticalAlignment="Bottom" Foreground="#55aaff"></Label>
                                                <Image Name="BathIcon2" Grid.Column="3" HorizontalAlignment="Left" Source="BathroomIcon.png" Margin="10 5 10 0"></Image>
                                                <Label Name="SizeNumber2" Grid.Column="4" FontSize="30" FontWeight="Normal" HorizontalAlignment="Right" VerticalAlignment="Bottom" Foreground="Black"></Label>
                                            </Grid>

                                        </Grid>

                                        <Grid Name ="Listing3" MouseLeftButtonDown="expandListing">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="350"></ColumnDefinition>
                                                <ColumnDefinition Width="50"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="250"/>
                                                <RowDefinition Height="100"/>
                                                <RowDefinition Height="50"/>
                                            </Grid.RowDefinitions>

                                            <Rectangle Grid.Row="0" Grid.Column="0" Fill="#eee" Height="250" Width="350" HorizontalAlignment="Left" Stroke="Black" VerticalAlignment="Top"/>
                                            <Rectangle Grid.Row="1" Grid.Column="0" Fill="#fff" Height="100" Width="350" HorizontalAlignment="Left" Stroke="Black" VerticalAlignment="Top"/>
                                            <Image Name="HouseImage3" Grid.Column="0" Margin="25"/>
                                            <Label Name="PriceNum3" Grid.Row="1" Grid.Column="0" FontSize="25" FontWeight="Black" Margin="8" HorizontalAlignment="Right" Content="{Binding Path=price[0], UpdateSourceTrigger=PropertyChanged}"></Label>
                                            <Label Name="Neighbourhood3" Grid.Row="1" Grid.Column="0" FontSize="25" FontWeight="Bold" Margin="8" HorizontalAlignment="Left" Content="{Binding Path=neighbourhood[0], UpdateSourceTrigger=PropertyChanged}"></Label>

                                            <Grid Name ="BedBath3" Grid.Row="1" Grid.Column ="0" HorizontalAlignment="center" VerticalAlignment="Bottom" Margin="8">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="30"/>
                                                    <ColumnDefinition Width="50"/>
                                                    <ColumnDefinition Width="40"/>
                                                    <ColumnDefinition Width="50"/>
                                                    <ColumnDefinition Width="150"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="50"/>
                                                </Grid.RowDefinitions>

                                                <Label Name="BedNumber3" Grid.Column="0" FontSize="30" FontWeight="Black" HorizontalAlignment="Right"  VerticalAlignment="Bottom" Foreground="#ff9900"></Label>
                                                <Image Name="BedIcon3" Grid.Column="1" HorizontalAlignment="Left" Source="BedroomIcon.png" Margin="10 5 10 0"></Image>
                                                <Label Name="BathNumber3" Grid.Column="2" FontSize="30" FontWeight="Black" HorizontalAlignment="Right" VerticalAlignment="Bottom" Foreground="#55aaff"></Label>
                                                <Image Name="BathIcon3" Grid.Column="3" HorizontalAlignment="Left" Source="BathroomIcon.png" Margin="10 5 10 0"></Image>
                                                <Label Name="SizeNumber3" Grid.Column="4" FontSize="30" FontWeight="Normal" HorizontalAlignment="Right" VerticalAlignment="Bottom" Foreground="Black"></Label>
                                            </Grid>


                                        </Grid>

                                        <Grid Name ="Listing4" MouseLeftButtonDown="expandListing">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="350"></ColumnDefinition>
                                                <ColumnDefinition Width="50"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="250"/>
                                                <RowDefinition Height="100"/>
                                                <RowDefinition Height="50"/>
                                            </Grid.RowDefinitions>

                                            <Rectangle Grid.Row="0" Grid.Column="0" Fill="#eee" Height="250" Width="350" HorizontalAlignment="Left" Stroke="Black" VerticalAlignment="Top"/>
                                            <Rectangle Grid.Row="1" Grid.Column="0" Fill="#fff" Height="100" Width="350" HorizontalAlignment="Left" Stroke="Black" VerticalAlignment="Top"/>
                                            <Image Name="HouseImage4" Grid.Column="0" Margin="25"/>
                                            <Label Name="PriceNum4" Grid.Row="1" Grid.Column="0" FontSize="25" FontWeight="Black" Margin="8" HorizontalAlignment="Right" Content="{Binding Path=price[0], UpdateSourceTrigger=PropertyChanged}"></Label>
                                            <Label Name="Neighbourhood4" Grid.Row="1" Grid.Column="0" FontSize="25" FontWeight="Bold" Margin="8" HorizontalAlignment="Left" Content="{Binding Path=neighbourhood[0], UpdateSourceTrigger=PropertyChanged}"></Label>

                                            <Grid Name ="BedBath4" Grid.Row="1" Grid.Column ="0" HorizontalAlignment="center" VerticalAlignment="Bottom" Margin="8">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="30"/>
                                                    <ColumnDefinition Width="50"/>
                                                    <ColumnDefinition Width="40"/>
                                                    <ColumnDefinition Width="50"/>
                                                    <ColumnDefinition Width="150"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="50"/>
